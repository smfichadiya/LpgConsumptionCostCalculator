<configuration>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />

	<add key="okta:ClientId" value="yourClientId" />
	<add key="okta:ClientSecret" value="yourClientSecret" />
	<add key="okta:OktaDomain" value="yourOktaDomain" />
	<add key="okta:Token" value="yourOktaToken" />

    <add key="okta:RedirectUri" value="https://localhost:44396/authorization-code/callback" />
    <add key="okta:PostLogoutRedirectUri" value="https://localhost:44396/Account/PostLogout" />
    
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.7.2" />
    <httpRuntime targetFramework="4.7.2" />
    <globalization culture="en-US" uiCulture="en-US"/>
    <customErrors mode="Off" />
  </system.web>
